{
  "name": "tripalcultivate/trpcultivatetheme",
  "type": "metapackage",
  "description": "Tripal Cultivate Theme provides default styling for the Drupal websites using the Tripal Cultivate family of packages.",
  "keywords": [
    "tripal",
    "drupal",
    "theme"
  ],
  "homepage": "https://github.com/TripalCultivate/TripalCultivate-Theme",
  "support": {
    "issues": "https://github.com/TripalCultivate/TripalCultivate-Theme/issues"
  },
  "license": "GPL-3.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Reynold Tan",
      "homepage": "https://github.com/reynoldtan",
      "role": "Lead Developer"
    },
    {
      "name": "Lacey-Anne Sanderson",
      "homepage": "https://github.com/laceysanderson/",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "drupal/core": "^10.0",
    "tripal/tripal": "^4.0-alpha1"
  },
  "scripts": {
    "livereload": "livereload --exclusions node_modules/ --exts 'css,js,twig,theme,apng,avif,gif,jpg,jpeg,jfif,pjpeg,pjp,png,svg,webp'"
  },
  "devDependencies": {
    "livereload": "^0.9.3"
  }
}
